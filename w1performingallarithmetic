#include<stdio.h>
int main(){
    int a,b;
    printf("enter two nhumber");
    scanf("%d %d",&a&b);

    sum=a+b;
    difference=a-b;
    product=a*b;
    quotient=a/b;
    remainder=a%b;

    printf("addition of two number is %d\n",sum);
    printf("subtraction of two number is %d\n",difference);
    printf("multiplication of two number is %d\n",product);
    printf("division of two number is %d\n",quotient);
    printf("modulus of two number is %d\n",remainder);
    return 0;
}